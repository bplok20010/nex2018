{
    "name": "nex",
    "version": "0.1.0",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf dist/* lib/* es/* amd/*",
        "minify": "uglifyjs dist/nex.js  -m -o dist/nex.min.js",
        "build:dist": "rollup -c",
        "build:cjs": "babel src -d lib",
        "build:es": "babel --no-babelrc src -d es",
        "build:amd": "babel --no-babelrc lib -d amd --plugins=transform-es2015-modules-amd",
        "build": "npm-run-all clean build:cjs build:amd",
        "css": "node-sass ./html/**/*.scss ./ --watch",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "nobo<nobo.zhou@foxmail.com>",
    "license": "MIT",
    "description": "",
    "devDependencies": {
        "babel-core": "^6.14.0",
        "babel-loader": "^6.2.5",
        "babel-plugin-istanbul": "^2.0.1",
        "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
        "babel-plugin-transform-es3-property-literals": "^6.22.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.23.0",
        "babel-preset-stage-0": "^6.22.0",
        "babel-runtime": "^6.22.0",
        "cross-env": "^3.1.4",
        "gulp": "^3.9.1",
        "gulp-sass": "^3.1.0",
        "npm-run-all": "^4.0.2",
        "rimraf": "^2.6.1",
        "rollup": "^0.41.4",
        "rollup-plugin-babel": "^2.7.1",
        "rollup-plugin-json": "^2.1.0",
        "uglify-js": "^2.7.5"
    },
    "dependencies": {}
}